{
    "type": "package",
    "name": "myrho/numeral-elm",
    "summary": "Elm module for (advanced) number formatting. Forked from ggb/numeral-elm",
    "license": "MIT",
    "version": "1.0.0",
    "exposed-modules": [
        "Numeral",
        "Language",
        "Languages.BritishEnglish",
        "Languages.English",
        "Languages.French",
